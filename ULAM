#include <stdio.h>
#include <stdlib.h>

int main()
{
   int a= 0,b=0,c = 0,d = 0,e = 0;
   scanf("%d",&a);
   while(a != 1)
   {
      if(a % 2 == 1)
      {
          printf("%d ",a);
          a = (a*3) +1;

      }
      else
      {
           printf("%d ",a);
          a = a/2;
      }
   }
   printf("1");

    return 0;
}
