#include <stdio.h>
#include <stdlib.h>


int main()
{

    long long int a,b,c=0,d,e=0,f=0,g=0,h=0,i=0;

    scanf("%lld",&a);

    for(b =0;b < a;b++)
    {
        scanf("%lld",&h);
        if ( b == 0)
           f = h;

        c +=h;
        d = c/(b+1);

        if(d < f)

              f = d;

    }
    printf("%lld",f);


    return 0;
