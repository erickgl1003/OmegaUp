#include <stdio.h>
#include <stdlib.h>

int main()
{
    char x;
    int z[10000];
    int y[10000];
    int a=1,b=0,c=0,d=0,e=0,f=0,g=0,h=0,i=0,j=0;

    scanf("%d %d",&a,&b);

    while(f ==0)
    {
        if(a >b)
        {
            c += a/b;
            a = a%b;
            if(a == 0) break;
        }
        else if(a ==b)
        {
            c += 1;
            break;
        }
        else{
            c+= b/a;
            b = b%a;
            if(b == 0)
                break;
        }

    }

    printf("%d",c);
    return 0;
}
